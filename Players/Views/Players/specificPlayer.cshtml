@{
  Layout = "_Layout";
}
    @* <p>this file is players/specificPlayer.cshtml</p> *@
@using Ffplayer.Models;
    <h1>Player Stats in 2022</h1>
    <table>
      <thead>
        <tr>
          <th>Date</th>
          <th>Week</th>
          <th>Team</th>
          <th>Opp</th>
          <th>Carries</th>
          <th>Targets</th>
          <th>Tds</th>
        </tr>
      </thead>
      <tbody>
        @foreach (Stats temp in Model)
        {
          <tr>
            <td>@temp.Date</td>
            <td>@temp.Week</td>
            <td>@temp.Team</td>
            <td>@temp.Opp</td>
            <td>@temp.Carries</td>
            <td>@temp.Targets</td>
            <td>@temp.Tds</td>
          </tr>
        }
      </tbody>
    </table>